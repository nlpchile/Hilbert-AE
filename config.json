{
    "language_model_dataset": {
        "filename": "./data/raw/horoscopo_raw.txt",
        "separator": " ",
        "max_number_of_examples": -1
    },
    "hilbert_mapper": {
        "order": 8,
        "number_of_channels": 13964
    },
    "process_file": {
        "vocabulary_size": 13964,
        "output_folder": "./data/binary/",
        "output_filename": "dataset_HDF5.h5",
        "order": 8,
        "name": "hilbert",
        "dtype": "int32"
    },
    "build_dataloader_from_disk": {
        "filename": "./data/binary/dataset_HDF5.h5",
        "batch_size": 32,
        "shuffle": true,
        "dev_split": 0.1,
        "num_workers": 2,
        "drop_last": true
    },
    "autoencoder": {
        "nc": 13964,
        "ndf": 256
    },
    "optimizer": {
        "lr": 1e-4,
        "betas": [
            0.9,
            0.999
        ],
        "eps": 1e-8,
        "weight_decay": 0.0,
        "amsgrad": false
    },
    "loss": {
        "size_average": null,
        "reduce": null,
        "reduction": "mean"
    },
    "seed": 42,
    "device": null,
    "enforce_reproducibility": false,
    "epochs": 100,
    "path_to_checkpoints": "./checkpoints/",
    "path_to_tensorboard_logs": "./tensorboard_logs/"
}